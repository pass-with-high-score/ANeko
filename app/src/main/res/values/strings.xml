    <string name="permission_onboarding_page2_primary">Continue</string>
    <string name="permission_onboarding_page3_title">Step 2: Enable overlay permission</string>
    <string name="permission_onboarding_page3_body">Turn on "Allow display over other apps". Then return to ANeko ✨</string>
