    <string name="permission_onboarding_appbar_title">Permissions</string>
    <string name="permission_onboarding_skip">Maybe later</string>
    <string name="permission_onboarding_page1_title">Let ANeko "appear" on your screen</string>
    <string name="permission_onboarding_page1_body">ANeko needs the "display over other apps" permission so the cat can show up anywhere you use.</string>
    <string name="permission_onboarding_page1_primary">Continue</string>
    <string name="permission_onboarding_page1_info_title">Why is this permission needed?</string>
    <string name="permission_onboarding_page1_info_body1">So the character can appear over other apps.</string>
    <string name="permission_onboarding_page1_info_body2">You can turn it off anytime in Settings.</string>
